{
  "dependencies": {
    "axios": "^0.19.2",
    "expo": "37.0.11",
    "jest": "^26.0.1",
    "jest-expo": "^25.1.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-elements": "^2.0.1",
    "react-native-web": "0.11.7",
    "react-test-renderer": "^16.13.1",
    "standard": "^14.3.4"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "eslint-plugin-react-hooks": "^4.0.4"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "/node_modules/@react-native-community/async-storage/(?!(lib))"
    ],
    "collectCoverageFrom": [
      "./**/*.{js,jsx}"
    ]
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo web",
    "eject": "expo eject",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "yarn test -- --coverage"
  },
  "private": true
}
